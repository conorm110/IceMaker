{
  "name": "icemaker",
  "displayName": "IceMaker",
  "description": "Verilog editor for iCE40 FPGAs using DFU",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:icemaker.uploadToFomu",
    "onCommand:icemaker.generateOutputevt1",
    "onCommand:icemaker.generateOutputevt2",
    "onCommand:icemaker.generateOutputevt3",
    "onCommand:icemaker.generateOutputpvt",
    "onCommand:icemaker.generateOutputhacker",
    "onCommand:icemaker.cleanProject",
    "onCommand:icemaker.newProject"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "icemaker.generateOutputevt1",
        "title": "IceMaker: Synthesize, PNR, and Generate Bitstream for EVT1"
      },
      {
        "command": "icemaker.generateOutputevt2",
        "title": "IceMaker: Synthesize, PNR, and Generate Bitstream for EVT2"
      },
      {
        "command": "icemaker.generateOutputevt3",
        "title": "IceMaker: Synthesize, PNR, and Generate Bitstream for EVT2"
      },
      {
        "command": "icemaker.generateOutputpvt",
        "title": "IceMaker: Synthesize, PNR, and Generate Bitstream for PVT"
      },
      {
        "command": "icemaker.generateOutputhacker",
        "title": "IceMaker: Synthesize, PNR, and Generate Bitstream for HACKER"
      },
      {
        "command": "icemaker.uploadToFomu",
        "title": "IceMaker: Upload Project to FOMU"
      },
      {
        "command": "icemaker.newProject",
        "title": "IceMaker: Set Up New Project in Current Directory"
      },
      {
        "command": "icemaker.cleanProject",
        "title": "IceMaker: Remove Old Output Files"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
